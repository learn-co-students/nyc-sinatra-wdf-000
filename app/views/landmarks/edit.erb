<h1>Edit <%= @landmark.name %></h1>

<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">

  <h4>Name: <input type="text" id="name" name="landmark[name]" value= "<%= @landmark.name %>"></h4>
  <h4>Year Built: <input type="text" id="year_completed" name="landmark[year_completed]" value="<%=@landmark.year_completed.to_s%>"></h4>
  <input type="submit" value="Edit Landmark">
</form>

